{
	int a, b, c, r;
	cout << "Vvedite coordinaty a, b, c" << endl;
	cin >> a;
	cin >> b;
	cin >> c;
	cout << abs(a-c);
	cout << " ";
	cout << abs(b-c);
	cout << " ";
	cout << abs(b-c)+abs(a-c);
	system("pause");
